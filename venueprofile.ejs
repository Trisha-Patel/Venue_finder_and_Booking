<!-- end theme style -->
</head>
<!-- end head -->
<!--body start-->
<body>

<!--  top bar -->
<%- include('utop') -%> 
<!--  end navigation -->
<!-- page banner -->
  <section id="page-banner" class="page-banner" style="background-image: url('userside/images/back3.jpg');"> 
    <div class="overlay-bg"></div>
    <div class="container">
      <div class="banner-dtl text-center">
        <h2 class="banner-heading">Vendor Profile</h2>
        <div class="breadcrumb-block">
          <ol class="breadcrumb">
            <li><a href="dashboard">Home</a></li>
            <li class="active">Vendor Profile</li>
          </ol>
        </div>
      </div>
    </div>
  </section>
<!-- end page banner -->
<!-- vendor profile -->
  <section id="vendor-profile" class="vendor-profile-main-page">
    <div class="container">
      <div class="row">
        <div class="col-md-8">
          <div class="vendor-pro-block">
            <div class="vendor-pro-info">
              <div class="row">
                <div class="col-md-5">
                  
                </div>
               </div>
            </div>
           </div>
        </div>
       </div>
      <div class="vendor-profile-listing-block">
        <div class="row">
          <% for(var i=0; i<ve.length ; i++){ %>
          <div class="col-md-4 col-sm-6">
            <div class="feature-block bline">
              <div class="feature-img">
                <h3 class="vendor-profile-listing-heading"><%= ve[i].vetitle %></h3>
                <a href="#"><img src="/upload/<%= ve[i].vephoto %>" height="200px"></a>
              </div>
              <div class="tags tags-clr-one">Featured</div>
              <div class="feature-dtl">
                <h6 class="feature-heading"><%= ve[i].vephoto %></a></h6>
                <p><i class="fa fa-map-marker" aria-hidden="true"></i> Street Address, <%= ve[i].veaddress %></i></p>
              </div>
              <div class="row">
                <div class="col-xs-6">
                  <div class="feature-vendor">
                    <a href="#">Venue price</a>
                  </div>
                </div>
                <div class="col-xs-6">
                  <div class="feature-price text-right">
                    $<%= ve[i].veprice %>
                    <% } %>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
<!-- end vendor profile -->
<!-- footer -->
<%- include('ufooter'); -%>
<!-- end footer -->
<!-- jquery -->
<script type="text/javascript" src="userside/js/jquery.min.js"></script>  <!-- jquery library js -->
<script type="text/javascript" src="userside/js/bootstrap.min.js"></script> <!-- bootstrap js -->
<script type="text/javascript" src="userside/js/owl.carousel.js"></script> <!-- owl carousel js -->
<script type="text/javascript" src="js/jquery.ajaxchimp.js"></script> <!-- mail chimp js -->
<script type="text/javascript" src="userside/js/smooth-scroll.js"></script> <!-- smooth scroll js -->
<script type="text/javascript" src="userside/js/jquery.magnific-popup.min.js"></script> <!-- magnify popup js --> 
<script type="text/javascript" src="userside/js/waypoints.min.js"></script> <!-- facts count js required for jquery.counterup.js file -->
<script type="text/javascript" src="userside/js/jquery.counterup.js"></script> <!-- facts count js-->
<script type="text/javascript" src="userside/js/menumaker.js"></script> <!-- menu js--> 
<script type="text/javascript" src="userside/js/jquery.share-tooltip.js"></script> <!-- share tooltip js--> 
<script type="text/javascript" src="userside/js/price-slider.js"></script> <!-- price slider / filter js-->
<script type="text/javascript" src="userside/js/bootstrap-datepicker.js"></script> <!-- datepicker js-->
<script type="text/javascript" src="userside/js/theme.js"></script> <!-- custom js -->  
<!-- end jquery -->
</body>
